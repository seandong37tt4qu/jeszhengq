version = "1.0.0"

measurements:
(
    {
        name: "system_vmstat",
        fields:
        (
            {
                description: "nr_free_pages",
                type: "counter",
                name: "nr_free_pages",
            },
            {
                description: "nr_zone_inactive_anon",
                type: "counter",
                name: "nr_zone_inactive_anon",
            },
            {
                description: "nr_zone_active_anon",
                type: "counter",
