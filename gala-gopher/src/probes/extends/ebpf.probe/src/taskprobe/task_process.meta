version = "1.0.0"
measurements:
(
    {
        name: "process",
        fields:
        (
            {
                description: "task group id of thd task",
                type: "key",
                name: "tgid",
            },
            {
                description: "pid of process group",
                type: "label",
                name: "pgid",
            },
            {
                description: "comm of the task",
                type: "label",
                name: "comm",
            },
            {
                description: "exe_file of the task",
                type: "label",
                name: "exe_file",
            },
            {
                description: "exec_file of the task",
                type: "label",
                name: "exec_file",
            },
            {
                description: "fork count of the task",
                type: "counter",
                name: "fork_count",
            },
            {
                description: "bytes of task system call I/O read operation",
                type: "counter",
                name: "task_rchar_bytes",
            },
            {
                description: "bytes of task system call I/O write operation",
                type: "counter",
                name: "task_wchar_bytes",
            },
            {
                description: "count of task system call I/O read operation",
                type: "counter",
                name: "task_syscr_count",
            },
            {
                description: "count of task system call I/O write operation",
                type: "counter",
                name: "task_syscw_count",
            },
            {
                description: "bytes of task read operation from disk",
                type: "counter",
                name: "task_read_bytes",
            },
            {
                description: "bytes written to disk by task (expected)",
                type: "counter",
                name: "task_write_bytes",
            },
            {
                description: "bytes written to disk cancelled by task",
                type: "counter",
                name: "task_cancelled_write_bytes",
            }
        )
    }
)
