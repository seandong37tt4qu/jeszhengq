version = "1.0.0"
measurements:
(
    {
        name: "thread",
        fields:
        (
            {
                description: "pid of the task",
                type: "key",
                name: "pid",
            },
            {
                description: "task group id of thd task",
                type: "label",
                name: "tgid",
            },
            {
                description: "comm of the task",
                type: "label",
                name: "comm",
            },
            {
                description: "disk major of the task io operate",
                type: "label",
                name: "major",
            },
            {
                description: "disk minor of the task io operate",
                type: "label",
                name: "minor",
            },
            {
                description: "fork count of the task",
                type: "counter",
                name: "fork_count",
            },
            {
                description: "io waite time of task",
                type: "counter",
                name: "task_io_wait_time_us",
            },
            {
                description: "count of task io operatation",
                type: "counter",
                name: "task_io_count",
            },
            {
                description: "accumulated Time of task io operatation",
                type: "counter",
                name: "task_io_time_us",
            },
            {
                description: "count of task I/O hang event",
                type: "counter",
                name: "task_hang_count",
            },
            {
                description: "count of task TCP/IP drop sk_buff event",
                type: "counter",
                name: "kfree_skb_cnt",
